@import '~app/assets/stylus';


.root
  content: ''

.heading
  font-size: 48px
  margin-bottom: 16px

.text
  margin-bottom: 8px
  text-indent: 2em

.subheading
  font-size: 32px
  margin-bottom: 8px

.repos
  margin-bottom: 16px

.repo
  &:not(:last-child)
    margin-bottom: 8px

.slide
  composes: fit from 'app/assets/stylus/common.styl'
  position: absolute
  top: 50%
  left: 50%
  transform: translate(-50%, -50%)
